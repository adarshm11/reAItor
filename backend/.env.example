# Anthropic API (Required)
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_api_key_here

# External Data APIs (Optional - falls back to mock data if not provided)
# Walk Score API - provides walkability, transit, and bike scores
# Get from: https://www.walkscore.com/professional/api.php
WALKSCORE_API_KEY=your_api_key_here

# GreatSchools API - provides nearby school ratings
# Get from: https://www.greatschools.org/api/
GREATSCHOOLS_API_KEY=your_api_key_here

# Google Maps API - provides geocoding, transit, and places data
# Get from: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=your_api_key_here

# Note: Crime data API integration is TODO
# Future: FBI Crime Data API or CrimeReports.com

# Application Settings
DEBUG=True
HOST=0.0.0.0
PORT=8000

# CORS Settings
ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000
